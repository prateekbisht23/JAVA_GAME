package com.prateekbisht.gaming.sprites;

import javax.swing.ImageIcon;

public class Enemy extends Sprite{
	
	public Enemy(int x, int speed) {
		w = 100;
		h = 100;
		this.x = x;
		this.speed = speed;
		y = 50;
		image = new ImageIcon(Player.class.getResource("spider.gif"));
	}
	
	public void move() {
		y = y + speed;
	}
	
}
